    <link href="@Url.Content("~/css/contactstyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/faqstyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/single.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/medile.css")" rel="stylesheet" type="text/css" />

    <!-- banner-slider -->
    <link href="@Url.Content("~/css/jquery.slidey.min.css")" rel="stylesheet" type="text/css" />
    <!-- //banner-slider -->
    <!-- pop-up -->
    <link href="@Url.Content("~/css/popuo-box.css")" rel="stylesheet" type="text/css" />
    <!-- //pop-up -->
    <!-- font-awesome icons -->
    <link href="@Url.Content("~/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <!-- //font-awesome icons -->
    <!-- js -->
    <script type="text/javascript" src="@Url.Content("~/js/jquery-2.1.4.min.js")"></script>
    <!-- //js -->
    <!-- banner-bottom-plugin -->
    <link href="@Url.Content("~/css/owl.carousel.css")" rel=" stylesheet" type="text/css" media="all">
    <script src="@Url.Content("~/js/owl.carousel.js")"></script>
    <script>
        $(document).ready(function () {
            $("#owl-demo").owlCarousel({

                autoPlay: 3000, //Set AutoPlay to 3 seconds

                items: 5,
                itemsDesktop: [640, 4],
                itemsDesktopSmall: [414, 3]

            });

        });
    </script>
